import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        long num=sc.nextLong();
        long rem,sum=0;
        while(num!=0){
            rem=num%10;
            sum=sum*10+rem;
            num=num/10;
            
        }
        System.out.println(sum);
    }
}
