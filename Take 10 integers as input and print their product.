import java.util.Scanner;

public class ProductOfTenNumbers {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        long product = 1;   
        
        for (int i = 1; i <= 10; i++) {
            int num = sc.nextInt();
            product = product * num;
        }
        
        System.out.println(product);
    }
}
