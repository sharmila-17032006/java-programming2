import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int num =sc.nextInt();
        int product=1,rem;
        while(num!=0){
            rem=num%10;
            num=num/10;
            product=product*rem;
            
        }
        System.out.println(product);
    
    }
}
